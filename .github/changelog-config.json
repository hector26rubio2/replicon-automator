  "ignore_labels": ["ignore-changelog"],
  "sort": {
    "order": "ASC",
    "on_property": "mergedAt"
  },
  "template": "#{{CHANGELOG}}\n\n**Full Changelog**: #{{RELEASE_DIFF}}\n\n---\n_Released by GitHub Actions_",
